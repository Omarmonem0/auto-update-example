{
  "name": "auto-update-example",
  "version": "1.0.5",
  "description": "electron auto update example",
  "main": "main.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "electron .",
    "build": "electron-builder build --win  --publish never",
    "deploy": "electron-builder build --win --publish always"
  },
  "author": "Omar Ahmed<omarmone0@gmail.com>",
  "license": "ISC",
  "devDependencies": {
    "electron": "^10.1.2",
    "electron-builder": "^22.8.0"
  },
  "build": {
    "publish": [
      {
        "provider": "github",
        "owner": "omarmonem0",
        "repo": "auto-update-example"
      }
    ],
    "appId": "com.example.ElectronAutoUpdate",
    "linux": {
      "target": [
        "deb"
      ],
      "publish": [
        "github"
      ]
    },
    "win": {
      "target": "nsis",
      "publish": [
        "github"
      ]
    }
  },
  "dependencies": {
    "electron-updater": "^4.3.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Omarmonem0/auto-update-example.git"
  }
}